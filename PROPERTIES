tailor.actwin.cmds.actwin.map.select=['<CR>', '<2-LeftMouse>']
 
# Option: tailor-actwin-cmds-actwin-map-top
# description:
#   Mapping to goto the first line in the ActWin.
#   Called from ActWin.
# default value: ['gg', '1G', '<PageUp>']
# user value: <No Value Set>
tailor.actwin.cmds.actwin.map.top=['gg', '1G', '<PageUp>']
 
