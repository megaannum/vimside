# Option: tailor-uses-of-symbol-at-point-location
# parent: tailor-location-diff-file
# description:
#   How to jump to uses-of-symbol-at-point file and pos.
# default value: <No Value Set>
# user value: 'split_window'
tailor.uses.of.symbol.at.point.location='split_window'
 
# Option: tailor-uses-of-symbol-at-point-use-sign-kind-marker
# description:
#   Whether or not to use sign kind "marker" for current line.
# default value: '1'
# user value: <No Value Set>
tailor.uses.of.symbol.at.point.use.sign.kind.marker='1'
 
# Option: tailor-uses-of-symbol-at-point-use-signs
# description:
#   Whether or not to use signs when displaying uses.
# default value: '1'
# user value: <No Value Set>
tailor.uses.of.symbol.at.point.use.signs='1'
 
# Option: tailor-uses-of-symbol-at-point-window
# description:
#   Whether to use the quickfix window to display uses or if all
#   of the uses are in the same window use the locationlist 
#   window.
# default value: 'actwin'
# user value: <No Value Set>
tailor.uses.of.symbol.at.point.window='actwin'
 
# Option: vimside-hover-balloon-enabled
# description:
#   Is the GVim Symbol-name balloon enabled.
# default value: 1
# user value: 0
vimside.hover.balloon.enabled=0
 
# Option: vimside-hover-term-balloon-enabled
# description:
#   Is the Vim Symbol-name term balloon enabled.
# default value: 1
# user value: 0
vimside.hover.term.balloon.enabled=0
 
# Option: vimside-java-version
# description:
#   Supported Java versions.
# default value: '1.6'
# user value: '1.7'
vimside.java.version='1.7'
 
# Option: vimside-log-enabled
# description:
#   Used to enable/disable Vimside logging.
# default value: 0
# user value: 1
vimside.log.enabled=1
 
# Option: vimside-log-file-name
# description:
#   File name of Vimside Log File.
#   If "vimside_log_file_path" is set, then this option
#   is ignored.
# default value: 'VIMSIDE_LOG'
# user value: <No Value Set>
vimside.log.file.name='VIMSIDE_LOG'
 
# Option: vimside-log-file-path
# description:
#   Full path name of Vimside Log File.
#   Both the parent Directory and File must be writeable.
#   If not set, then either the Ensime Config File Directory is.
#   is used or the Current Working Directory (CWD). The CWD is
#   used if "use_cwd_as_default_output_dir" to true (1).
# default value: <No Value Set>
# user value: <No Value Set>
# vimside.log.file.path=<No Value Set>
 
# Option: vimside-log-file-use-pid
# description:
#   The Vim log file has the current process id (pid) as a prefix.
# default value: 0
# user value: <No Value Set>
vimside.log.file.use.pid=0
 
# Option: vimside-port-file-wait-time
# description:
#   How long to wait after starting the Ensime Server before
#   attempting to read the file containing the port number.
# default value: 4
# user value: <No Value Set>
vimside.port.file.wait.time=4
 
# Option: vimside-project-options-enabled
# description:
#   Load a project-local user options file.
# default value: 1
# user value: 1
vimside.project.options.enabled=1
 
# Option: vimside-project-options-file-name
# description:
#   File name of a project-local options file.
# default value: 'options_project.vim'
# user value: <No Value Set>
vimside.project.options.file.name='options_project.vim'
 
# Option: vimside-scala-version
# description:
#   Supported Scala versions.
# default value: '2.9.2'
# user value: '2.10.0'
vimside.scala.version='2.10.0'
 
# Option: vimside-use-cwd-as-output-dir
# description:
#   If the option "vimside_log_file_path", then if true (1),
#   the Current Working Directory is used and if false (0), then
#   the Directory containing the Ensime Config File is used.
# default value: 0
# user value: <No Value Set>
vimside.use.cwd.as.output.dir=0
 
